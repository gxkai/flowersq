	<style type="text/css">
		
		body{
		 	font-family: "微软雅黑";
        	font-size: 0.8em;
        	color:#000;
			background: #fff;
			margin: 0 auto;
			padding: 0;
			max-width: 763px;
			height:100%;
			background-color: #eeeeee;
		}
		p{
			padding: 0;
			margin: 0;
		}
		.top {
			background: #f9f9f9;
			text-align: center;
			padding: 5vw;
		}
		.top .nick{
			margin: 3% 0;
		}
		.headimg img{
			width: 70px;
			height: 70px;
			border-radius: 100%;	
			vertical-align: middle;
		}
		.main_info {
			background: #fff;
			display: flex;
			width: 100%;
			justify-content: center;
  			align-items: center;
  			margin-bottom: 10px;
		}
		.main_info_span{
			width: 25%;
			padding: 12px 0;
			display: flex;
			flex-direction: column;
			justify-content: space-between;
  			align-items: center;
		}
		.main_change_span.on{
			border-bottom: 2px solid rgb(80, 210, 194);
		}
		.main_change_span i{
			color: #c5c3c3;
		}
		.main_change_span.on i{
			color: #50d2c2;
		}
		.main_info_span span:FIRST-CHILD{
			font-weight: bold;
			font-size: 1.2em;
		}
		.main_info_span span:NTH-CHILD(2){
			font-size: 0.7em;
			color: #b7b7b7;
		}
		
		.main_change {
			background: #fff;
			display: flex;
			width: 100%;
			justify-content: center;
  			align-items: center;
		}
		.main_change_span{
			width: 24%;
    		padding: 10px 2px;
    		text-align: center;
  			align-items: center;
		    margin: 10px 12px 0;
		}
		
		.main_talk{
			background: #f9f9f9;
			height: 150px;
			margin: 10px 0;
		}
		
		.layui-m-layer-footer .layui-m-layercont{
			padding:0;
		}
		.load{
			text-align: center;
			margin: 20px 0;
		}
	</style>
    
	<style type="text/css">
		.head_span img{
			width: 20px;
			height: 20px;
			border-radius: 15px;	
		}
		.main_show_part{
			background-color: #fff;
			margin-bottom: 3vw;
			padding: 3vw;
		}
		.main_show_part:FIRST-CHILD{
			padding-top: 20px;
		}
		
		.grade{
			border: 1px solid #50d2c2; 
	    	border-radius: 10px;
    		padding: 0px 6px;
		    color: #50d2c2;
		    margin:0 5px;
		    font-size: 0.5em;
		}
		.element{
			margin: auto;
		}
		.element:after { 
			content: ''; 
			display: table; 
			clear: both;
		}
		.element figure { 
			float: left; 
			display: inline;
			margin: 4px;
		}
		.element img{
		    position: relative;
		    width: 100%;
		    height: 0;
		    overflow: hidden;
		    margin: 0;
		    padding-bottom: 100%;
		    background-position: center;
		    background-repeat: no-repeat;
		    background-size: cover;
		}
		
		.icon{
			overflow: hidden;
			margin: 10px 0;
		}
		.icon .span1,.icon .span2{
			display: inline-block;
			vertical-align: middle;
		}
		.icon .span1.on{
			color: #f55500;
		}
		.icon img{
			width: 20px;
    		vertical-align: middle;
		}
		.icon span{
			width: 30%;
			padding: 0 3vw;
		}
		.main_show_body .content{
			margin: 1%;
			overflow: hidden;
		    text-overflow: ellipsis;
		    display: -webkit-inline-box;
		    -webkit-line-clamp: 3;
		    -webkit-box-orient: vertical;
		    vertical-align: middle;
		}
		.main_show_body .content b{
			vertical-align: middle;
		}
		.main_show_bottom{
			background-color: #f9f9f9;
			padding: 1% 4%;
			font-size: 0.9em;
		}
		.main_show_bottom .com_con{
			font-size: 1.1em;
		}
		
